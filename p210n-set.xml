		<tags>
			<tag>transport</tag>
			<tag>cessna</tag>
			<tag>1960s</tag>
			<tag>high-wing</tag>
			<tag>ifr</tag>
			<tag>single-engine</tag>
		</tags>
		<model>
			<path>Models/p210n.xml</path>
			<livery>
				<file type="string">Default</file>
			</livery>
		</model>
		<menubar include="gui/p210n-menu.xml"></menubar>
		<help include="p210n-help.xml"></help>
		<view n="0">
			<name>Cockpit</name>
			<type>lookfrom</type>
			<internal type="bool">true</internal>
			<config>
				<x-offset-m type="double">-0.3</x-offset-m>
				<y-offset-m type="double">0.65</y-offset-m>
				<z-offset-m type="double">0.3</z-offset-m>
				<pitch-offset-deg>3</pitch-offset-deg>
				<default-field-of-view-deg type="double">65.0</default-field-of-view-deg>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<ground-level-nearplane-m type="double">6f</ground-level-nearplane-m>
				<limits>
					<enabled type="bool">false</enabled>
					<left>
						<heading-max-deg type="double">140</heading-max-deg>
						<x-offset-max-m type="double">0.15</x-offset-max-m>
						<x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
					</left>
					<right>
						<heading-max-deg type="double">140</heading-max-deg>
						<x-offset-max-m type="double">0.15</x-offset-max-m>
						<x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
					</right>
				</limits>
			</config>
			<dynamic>
				<enabled type="bool">false</enabled>
			</dynamic>
		</view>
	</sim>
	
	<nasal>
		<p210n>
			<file>Nasal/autostart.nas</file>
			<file>Nasal/rain.nas</file>
			<file>Nasal/liveries.nas</file>
			<file>Nasal/doors.nas</file>
		</p210n>
	</nasal>
	
	<payload>
		<weight>
			<name type="string">Pilot</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[0]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Co-Pilot</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Middle left passenger</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[2]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Middle right passenger</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Rear left passenger</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[4]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Rear right passenger</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[5]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">220.0</max-lb>
		</weight>
		<weight>
			<name type="string">Baggage A</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[6]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">200.0</max-lb>
		</weight>
		<weight>
			<name type="string">Baggage B</name>
			<weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[7]"/>
			<min-lb type="double">0.0</min-lb>
			<max-lb type="double">80.0</max-lb>
		</weight>
	</payload>

	<limits>
		<max-positive-g>3.8</max-positive-g>
		<max-negative-g>-1.52</max-negative-g>

		<vne>200</vne>

		<mass-and-balance>
			<maximum-ramp-mass-lbs>4016</maximum-ramp-mass-lbs>
			<maximum-takeoff-mass-lbs>4000</maximum-takeoff-mass-lbs>
			<maximum-landing-mass-lbs>3800</maximum-landing-mass-lbs>
			<!--<maximum-zero-fuel-mass-lbs>3475.1</maximum-zero-fuel-mass-lbs>-->
		</mass-and-balance>
		<max-flap-extension-speed>
			<flaps>0.3333</flaps>
			<speed>160</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>0.6666</flaps>
			<speed>130</speed>
		</max-flap-extension-speed>
		<max-flap-extension-speed>
			<flaps>1.0</flaps>
			<speed>115</speed>
		</max-flap-extension-speed>
		<max-gear-extension-speed>200</max-gear-extension-speed>
	</limits>
	
	<engines>
		<engine>
			<cowl-flaps-norm alias="/fdm/jsbsim/propulsion/engine[0]/cowl-flaps-norm"/>
		</engine>
	</engines>
	
	<consumables>
		<fuel>
			<tank n="0">
				<name>Left wing tank</name>
			</tank>
			<tank n="1">
				<name>Right wing tank</name>
			</tank>
			<tank n="2">
				<name>Left reservoir tank</name>
				<hidden>true</hidden>
			</tank>
			<tank n="3">
				<name>Right reservoir tank</name>
				<hidden>true</hidden>
			</tank>
		</fuel>
	</consumables>
	
	<controls>
		<gear>
			<brake-parking>true</brake-parking>
		</gear>
		<engines>
			<engine>
				<cowl-flaps-norm>1</cowl-flaps-norm>
				<propeller-pitch>1</propeller-pitch>
			</engine>
		</engines>
	</controls>
	
	<instrumentation>
		<heading-indicator>
			<heading-bug-deg>0</heading-bug-deg>
		</heading-indicator>
	</instrumentation>
</PropertyList>

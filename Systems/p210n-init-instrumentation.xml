<?xml version="1.0"?>
<PropertyList>
	<!-- most of this should be replaced by system files to support electrcial / other system failures -->
	<egt>
		<indicated-temperature-degf alias="/engines/engine/egt-degf"/>
	</egt>
	<map-fuelflow>
		<indicated-map-inhg alias="/engines/engine/mp-inhg"/>
		<indicated-fuelflow-pph alias="/engines/engine/fuel-flow_pph"/>
	</map-fuelflow>
	<vacuum>
		<indicated-suction-inhg alias="/systems/vacuum/suction-inhg"/>
	</vacuum>
	<tachometer>
		<indicated-engine-rpm alias="/engines/engine/rpm"/>
		<indicated-tach-time-hours alias="/fdm/jsbsim/propulsion/engine/tach-time-hours"/>
	</tachometer>
	<altimeter n="1">
		<serviceable type="bool">true</serviceable>
	</altimeter>
	
	<!-- radios -->
	<comm n="0">
		<frequencies>
			<selected-mhz type="double">118.000</selected-mhz>
			<selected-mhz-fmt type="string">118.00</selected-mhz-fmt>
			<memory n="0" type="double">118.000</memory>
			<memory n="1" type="double">118.000</memory>
			<memory n="2" type="double">118.000</memory>
			<current-memory type="int">0</current-memory>
		</frequencies>
	</comm>
	<comm n="1">
		<frequencies>
			<selected-mhz type="double">118.000</selected-mhz>
			<selected-mhz-fmt type="string">118.00</selected-mhz-fmt>
			<memory n="0" type="double">118.000</memory>
			<memory n="1" type="double">118.000</memory>
			<memory n="2" type="double">118.000</memory>
			<current-memory type="int">0</current-memory>
		</frequencies>
	</comm>
	<nav n="0">
		<frequencies>
			<selected-mhz type="double">108.000</selected-mhz>
			<selected-mhz-fmt type="string">108.00</selected-mhz-fmt>
			<memory n="0" type="double">108.000</memory>
			<memory n="1" type="double">108.000</memory>
			<memory n="2" type="double">108.000</memory>
			<current-memory type="int">0</current-memory>
		</frequencies>
	</nav>
	<nav n="1">
		<frequencies>
			<selected-mhz type="double">108.000</selected-mhz>
			<selected-mhz-fmt type="string">108.00</selected-mhz-fmt>
			<memory n="0" type="double">108.000</memory>
			<memory n="1" type="double">108.000</memory>
			<memory n="2" type="double">108.000</memory>
			<current-memory type="int">0</current-memory>
		</frequencies>
	</nav>
	<marker-beacon>
		<test type="bool">false</test>
		<inner type="bool">false</inner>
		<middle type="bool">false</middle>
		<outer type="bool">false</outer>
	</marker-beacon>
	
	<dme>
		<hold-frequency-mhz>108.000</hold-frequency-mhz>
		<mode>0</mode> <!-- OFF -->
		<display-mode>0</display-mode> <!-- TTS -->
		<test type="bool">false</test>
		<power-btn type="bool">false</power-btn>
		<source-knob>2</source-knob> <!-- NAV1 -->
		<source>/instrumentation/nav/frequencies/selected-mhz</source>
	</dme>
	
	<rnav>
		<selected-bearing-deg>0.0</selected-bearing-deg>
		<selected-bearing-deg-hundreds>0</selected-bearing-deg-hundreds>
		<selected-bearing-deg-tenths>0</selected-bearing-deg-tenths>
		<selected-bearing-deg-units>0</selected-bearing-deg-units>
		<selected-bearing-deg-decimals>0</selected-bearing-deg-decimals>
		<selected-distance-nm>0.0</selected-distance-nm>
		<selected-distance-nm-hundreds>0</selected-distance-nm-hundreds>
		<selected-distance-nm-tenths>0</selected-distance-nm-tenths>
		<selected-distance-nm-units>0</selected-distance-nm-units>
		<selected-distance-nm-decimals>0</selected-distance-nm-decimals>
		<waypoint n="0">
			<bearing-deg>0</bearing-deg>
			<distance-nm>0</distance-nm>
		</waypoint>
		<waypoint n="1">
			<bearing-deg>0</bearing-deg>
			<distance-nm>0</distance-nm>
		</waypoint>
		<display-waypoint type="int">1</display-waypoint>
		<fly-waypoint type="int">1</fly-waypoint>
		<!-- sensitivity of the CDI needle 0=ENR (enroute), 1=APPR (approach) -->
		<sensitivity>0</sensitivity>
	</rnav>
</PropertyList>

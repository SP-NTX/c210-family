<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>c210n.ac</path>
	
	<!-- Livery over MP -->
	<animation>
		<type>material</type>
		<object-name>Fuselage</object-name>
		<object-name>Rudder</object-name>
		<object-name>NavAntennas</object-name>
		<object-name>LeftElevatorMount</object-name>
		<object-name>RightElevatorMount</object-name>
		<object-name>LeftElevator</object-name>
		<object-name>RightElevator</object-name>
		<object-name>ElevatorTrimTab</object-name>
		<object-name>PilotDoorExterior</object-name>
		<object-name>CopilotDoorExterior</object-name>
		<object-name>BaggageDoorExterior</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture-fuselage</texture-prop>
		<texture>fuselage.png</texture>
	</animation>
	<animation>
		<type>material</type>
		<object-name>RightWing</object-name>
		<object-name>LeftWing</object-name>
		<object-name>LeftFlap</object-name>
		<object-name>RightFlap</object-name>
		<object-name>LeftAileron</object-name>
		<object-name>LeftAileronTrimTab</object-name>s
		<object-name>RightAileron</object-name>
		<object-name>RightAileronTrimTab</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture-wings</texture-prop>
		<texture>wings.png</texture>
	</animation>
	<nasal>
		<load>
			<![CDATA[
				# called only when the model is loaded as an AI model (multiplayer)
				var livery_update = aircraft.livery_update.new("Aircraft/c210-family/Models/Liveries");
			]]>
		</load>
		<unload>
			<![CDATA[
				livery_update.stop();
			]]>
		</unload>
	</nasal>
	
	<!-- Control surfaces animations-->
	<animation>
		<type>rotate</type>
		<object-name>Rudder</object-name>
		<axis>
			<object-name>RudderAxis</object-name>
		</axis>
		<property>surface-positions/rudder-pos-norm</property>
		<factor>-27</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>LeftElevator</object-name>
		<axis>
			<object-name>LeftElevatorAxis</object-name>
		</axis>
		<property>surface-positions/elevator-pos-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>-23</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>-1</ind>
				<dep>17</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>RightElevator</object-name>
		<object-name>ElevatorTrimTab</object-name>
		<object-name>ElevatorTrimTabAxis</object-name>
		<axis>
			<object-name>RightElevatorAxis</object-name>
		</axis>
		<property>surface-positions/elevator-pos-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>23</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>-1</ind>
				<dep>-17</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>ElevatorTrimTab</object-name>
		<axis>
			<object-name>ElevatorTrimTabAxis</object-name>
		</axis>
		<property>controls/flight/elevator-trim</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>-25</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>-1</ind>
				<dep>10</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>LeftAileron</object-name>
		<object-name>LeftAileronTrimTab</object-name>
		<object-name>LeftAileronTrimTabAxis</object-name>
		<axis>
			<object-name>LeftAileronAxis</object-name>
		</axis>
		<property>surface-positions/left-aileron-pos-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>-20</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>-1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>RightAileron</object-name>
		<object-name>RightAileronTrimTab</object-name>
		<object-name>RightAileronTrimTabAxis</object-name>
		<axis>
			<object-name>RightAileronAxis</object-name>
		</axis>
		<property>surface-positions/right-aileron-pos-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>-20</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>-1</ind>
				<dep>15</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightAileronTrimTab</object-name>
		<axis>
			<object-name>RightAileronTrimTabAxis</object-name>
		</axis>
		<property>controls/flight/aileron-trim-norm</property>
		<factor>15</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>LeftAileronTrimTab</object-name>
		<axis>
			<object-name>LeftAileronTrimTabAxis</object-name>
		</axis>
		<property>controls/flight/aileron-trim-norm</property>
		<factor>15</factor>
	</animation>
	
	<!-- Propeller transparency at higher RPM -->
	<animation>
		<type>spin</type>
		<!--<object-name>PropellerSpinner</object-name>-->
		<object-name>PropellerBlades</object-name>
		<object-name>FastPropeller</object-name>
		<axis>
			<object-name>PropellerAxis</object-name>
		</axis>
		<property>engines/engine[0]/rpm</property>
	</animation>
	<animation>
		<type>select</type>
		<object-name>PropellerBlades</object-name>
		<condition>
			<less-than>
				<property>engines/engine[0]/rpm</property>
				<value>1200</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>FastPropeller</object-name>
		<condition>
			<greater-than>
				<property>engines/engine[0]/rpm</property>
				<value>600</value>
			</greater-than>
		</condition>
	</animation>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>FastPropeller</object-name>
	</effect>
	
	<!-- Wheels spinning when rolling and bumping when the ground is rough and nose wheel turning -->
	<animation>
		<type>rotate</type>
		<condition>
			<equals>
				<property>gear/gear[0]/position-norm</property>
				<value>1</value>
			</equals>
		</condition>
		<object-name>NoseWheel</object-name>
		<object-name>NoseWheelStrut</object-name>
		<axis>
			<object-name>NoseWheelSteeringAxis</object-name>
		</axis>
		<property>gear/gear[0]/steering-norm</property>
		<factor>27</factor>
	</animation>
	<animation>
		<type>translate</type>
		<condition>
			<equals>
				<property>gear/gear[0]/position-norm</property>
				<value>1</value>
			</equals>
		</condition>
		<object-name>NoseWheel</object-name>
		<object-name>NoseWheelRollingAxis</object-name>
		<axis>
			<object-name>NoseWheelSteeringAxis</object-name>
		</axis>
		<property>gear/gear[0]/compression-norm</property>
		<max>0.5</max>
		<factor>-0.18</factor>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>NoseWheel</object-name>
		<axis>
			<object-name>NoseWheelRollingAxis</object-name>
		</axis>
		<property>gear/gear[0]/rollspeed-ms</property>
	</animation>
	
	<animation>
		<type>translate</type>
		<object-name>RightWheel</object-name>
		<object-name>RightWheelRollingAxis</object-name>
		<axis>
			<object-name>RightWheelBumpingAxis</object-name>
		</axis>
		<property>gear/gear[1]/compression-norm</property>
		<max>1</max>
		<factor>0.2</factor>
		<condition>
			<equals>
				<property>gear/gear[1]/position-norm</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>RightWheel</object-name>
		<axis>
			<object-name>RightWheelRollingAxis</object-name>
		</axis>
		<property>gear/gear[1]/rollspeed-ms</property>
	</animation>

	
	<animation>
		<type>translate</type>
		<object-name>LeftWheel</object-name>
		<object-name>LeftWheelRollingAxis</object-name>
		<axis>
			<object-name>LeftWheelBumpingAxis</object-name>
		</axis>
		<property>gear/gear[2]/compression-norm</property>
		<max>1</max>
		<factor>0.2</factor>
		<condition>
			<equals>
				<property>gear/gear[2]/position-norm</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>LeftWheel</object-name>
		<axis>
			<object-name>LeftWheelRollingAxis</object-name>
		</axis>
		<property>gear/gear[2]/rollspeed-ms</property>
	</animation>
	
	<!-- Gear retraction / extension animations -->
	<animation>
		<type>rotate</type>
		<object-name>NoseWheelStrut</object-name>
		<object-name>NoseWheel</object-name>
		<axis>
			<object-name>NoseGearRetractionAxis</object-name>
		</axis>		
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.65</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.35</ind>
				<dep>-100</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>-100</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NoseGearLeftDoor</object-name>
		<axis>
			<object-name>NoseGearLeftDoorAxis</object-name>
		</axis>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>0.3</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>NoseGearRightDoor</object-name>
		<axis>
			<object-name>NoseGearRightDoorAxis</object-name>
		</axis>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>-70</dep>
			</entry>
			<entry>
				<ind>0.3</ind>
				<dep>-70</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>-0</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>NoseWheel</object-name>
		<object-name>NoseWheelStrut</object-name>
		<axis>
			<object-name>NoseWheelSteeringAxis</object-name>
		</axis>
		<!--<condition>
			<not>
				<equals>
					<property>gear/gear[0]/position-norm</property>
					<value>1</value>
				</equals>
			</not>
		</condition>-->
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.65</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.35</ind>
				<dep>90</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>RightWheelStrut</object-name>
		<object-name>RightWheel</object-name>
		<axis>
			<object-name>RightGearRetractionAxis</object-name>
		</axis>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>169</dep>
			</entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>LeftWheelStrut</object-name>
		<object-name>LeftWheel</object-name>
		<axis>
			<object-name>LeftGearRetractionAxis</object-name>
		</axis>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>1</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0</ind>
				<dep>-169</dep>
			</entry>
		</interpolation>
	</animation>
	
	<!-- Doors clickable and opening / closing -->
	 <animation>
		<type>pick</type>
		<object-name>PilotDoorInterior</object-name>
		<object-name>PilotDoorExterior</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					c210.baggageDoor.toggle();
				</script>
			</binding>
		</action>
	 </animation>
	 <animation>
	 	<type>rotate</type>
	 	<object-name>PilotDoorInterior</object-name>
	 	<object-name>PilotDoorExterior</object-name>
	 	<object-name>PilotWindowsExterior</object-name>
	 	<object-name>PilotWindowsInterior</object-name>
	 	<axis>
	 		<object-name>PilotDoorAxis</object-name>
	 	</axis>
	 	<property>sim/model/doors/pilot/position-norm</property>
	 	<factor>80</factor>
	 </animation>
	 <animation>
		<type>pick</type>
		<object-name>CopilotDoorInterior</object-name>
		<object-name>CopilotDoorExterior</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					c210.copilotDoor.toggle();
				</script>
			</binding>
		</action>
	 </animation>
	 <animation>
	 	<type>rotate</type>
	 	<object-name>CopilotDoorInterior</object-name>
	 	<object-name>CopilotDoorExterior</object-name>
	 	<object-name>CopilotWindowsExterior</object-name>
	 	<object-name>CopilotWindowsInterior</object-name>
	 	<axis>
	 		<object-name>CopilotDoorAxis</object-name>
	 	</axis>
	 	<property>sim/model/doors/copilot/position-norm</property>
	 	<factor>-80</factor>
	 </animation>
	 <animation>
		<type>pick</type>
		<object-name>BaggageDoorInterior</object-name>
		<object-name>BaggageDoorExterior</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					c210.baggageDoor.toggle();
				</script>
			</binding>
		</action>
	 </animation>
	 <animation>
	 	<type>rotate</type>
	 	<object-name>BaggageDoorInterior</object-name>
	 	<object-name>BaggageDoorExterior</object-name>
	 	<axis>
	 		<object-name>BaggageDoorAxis</object-name>
	 	</axis>
	 	<property>sim/model/doors/baggage/position-norm</property>
	 	<factor>90</factor>
	 </animation>
	 
	<!-- Rain drops, fog and frost effects on exterior glass - transparency weirdly is controlled by the alpha of the emission, just the below effect does NOT make the glass transparent !-->
	<effect>
		<inherits-from>Aircraft/c210-family/Effects/exterior-glass</inherits-from>
		 <object-name>WindscreenExterior</object-name>
		 <object-name>PilotWindowsExterior</object-name>
		 <object-name>CopilotWindowsExterior</object-name>
		 <object-name>LeftPassengerWindowsExterior</object-name>
		 <object-name>RightPassengerWindowsExterior</object-name>
		 <object-name>BackWindowsExterior</object-name>
	</effect>
	<effect>
		<inherits-from>Effects/glass</inherits-from>
		 <object-name>WindscreenInterior</object-name>
		 <object-name>PilotWindowsInterior</object-name>
		 <object-name>CopilotWindowsInterior</object-name>
		 <object-name>LeftPassengerWindowsInterior</object-name>
		 <object-name>RightPassengerWindowsInterior</object-name>
		 <object-name>BackWindowsInterior</object-name>
	</effect>
	
	<!-- Seats -->
	<model>
		<path>Aircraft/c210-family/Models/Interior/seat.ac</path>
		<offsets>
			<x-m>0.2</x-m>
			<y-m>-0.28</y-m>
			<z-m>-0.25</z-m>
		</offsets>
	</model>
	<model>
		<path>Aircraft/c210-family/Models/Interior/seat.ac</path>
		<offsets>
			<x-m>0.2</x-m>
			<y-m>0.28</y-m>
			<z-m>-0.25</z-m>
		</offsets>
	</model>
</PropertyList>

<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>c210n.ac</path>
	<animation>
		<type>rotate</type>
		<object-name>Rudder</object-name>
		<axis>
			<object-name>RudderAxis</object-name>
		</axis>
		<property>controls/flight/rudder</property>
		<factor>-30</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>LeftElevator</object-name>
		<axis>
			<object-name>LeftElevatorAxis</object-name>
		</axis>
		<property>controls/flight/elevator</property>
		<factor>-30</factor>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>RightElevator</object-name>
		<axis>
			<object-name>RightElevatorAxis</object-name>
		</axis>
		<property>controls/flight/elevator</property>
		<factor>30</factor>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>PropellerSpinner</object-name>
		<object-name>PropellerBlade1</object-name>
		<object-name>PropellerBlade2</object-name>
		<object-name>PropellerBlade3</object-name>
		<axis>
			<object-name>PropellerAxis</object-name>
		</axis>
		<property>engines/engine[0]/rpm</property>
	</animation>
	<animation>
		<type>select</type>
		<object-name>PropellerBlade1</object-name>
		<object-name>PropellerBlade2</object-name>
		<object-name>PropellerBlade3</object-name>
		<condition>
			<less-than>
				<property>engines/engine[0]/rpm</property>
				<value>1200</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>FastPropeller</object-name>
		<condition>
			<greater-than>
				<property>engines/engine[0]/rpm</property>
				<value>600</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>blend</type>
		<object-name>FastPropeller</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>0.000333333</factor>
		<min>0.2</min>
		<max>0.8</max>
	</animation>
</PropertyList>

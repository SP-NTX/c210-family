<?xml version="1.0"?>
<PropertyList>
	<path>alt-sel.ac</path>
	
	<animation>
		<type>knob</type>
		<object-name>Knob</object-name>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>controls/pressurization/selected-cabin-altitude-ft</property>
		<factor>0.0327</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/pressurization/selected-cabin-altitude-ft</property>
				<factor>100</factor>
				<min>0</min>
				<max>10000</max>
			</binding>
		</action>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>cabin-alt-sel</tooltip-id>
				<label>Target cabin altitude: %d feet</label>
				<measure-text>Target cabin altitude: 10000 feet</measure-text>
				<property>controls/pressurization/selected-cabin-altitude-ft</property>
			</binding>
		</hovered>
	</animation>
</PropertyList>

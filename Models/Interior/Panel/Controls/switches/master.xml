<?xml version="1.0"?>
<PropertyList>
	<path>red.ac</path>
	
	<params>
		<property>controls/engines/engine/master-alt</property>
	</params>
	
	<animation>
		<type>knob</type>
		<object-name>Switch</object-name>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
		<property alias="../../params/property"/>
		<factor>40</factor>
		<action>
			<binding>
				<command>property-toggle</command>
				<property alias="../../../../params/property"/>
			</binding>
		</action>
	</animation>
</PropertyList>

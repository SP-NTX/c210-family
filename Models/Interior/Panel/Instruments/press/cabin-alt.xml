<?xml version="1.0"?>
<PropertyList>
	<path>cabin-alt.ac</path>
	
	<animation>
		<type>rotate</type>
		<object-name>AltNeedle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/pressurization/indicated-cabin-altitude-ft</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>5000</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>10000</ind>
				<dep>130</dep>
			</entry>
			<entry>
				<ind>15000</ind>
				<dep>180</dep>
			</entry>
			<entry>
				<ind>20000</ind>
				<dep>220</dep>
			</entry>
			<entry>
				<ind>25000</ind>
				<dep>255</dep>
			</entry>
			<entry>
				<ind>30000</ind>
				<dep>285</dep>
			</entry>
			<entry>
				<ind>35000</ind>
				<dep>310</dep>
			</entry>
		</interpolation>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>PSINeedle</object-name>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<property>instrumentation/pressurization/indicated-differential-pressure-psi</property>
		<factor>30</factor>
		<min>0</min>
		<max>6</max>
	</animation>
	
	<effect>
		<inherits-from>Effects/glass</inherits-from>
		<object-name>Glass</object-name>
	</effect>
</PropertyList>

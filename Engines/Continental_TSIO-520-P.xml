<!--
  File:     Continental_TSIO-520-P.xml
  Author:   Dany93, TheEagle, S&J
  Inputs:
    name:           Continental_TSIO-520-P
    type:           piston
    power:          310 hp
-->

<piston_engine name="Continental_TSIO-520-P">

   <displacement unit="IN3"> 520.00 </displacement>
   <bore unit="IN">    5.25 </bore>
   <stroke unit="IN">  4.0 </stroke>
   <cylinders>         6.0  </cylinders>
   <compression-ratio>  7.5 </compression-ratio>
   <maxhp>        285.00 </maxhp>
   <cycles>         4.0 </cycles>
   <idlerpm>      600.0 </idlerpm>   
   <maxrpm>      2600.0 </maxrpm>   
   <minmp unit="INHG">         15 </minmp>
   <maxmp unit="INHG">         35.5 </maxmp>   
   <bsfc unit="LBS/HP*HR"> 0.3939 </bsfc>

   <starter-torque> 100.00 </starter-torque>   
   <static-friction> 1.55 </static-friction>
   <sparkfaildrop>  0.082 </sparkfaildrop>
   <!--<volumetric-efficiency> 0.7037 </volumetric-efficiency>-->
   <function name="volumetric-efficiency">
       <table>
           <independentVar>atmosphere/density-altitude</independentVar>
           <tableData>
                 0           0.7037
                1000     0.698
                5000     0.67
                10000   0.637
                15000   0.6069
                17000   0.5962
            </tableData>
        </table>
   </function>
   
   <ram-air-factor>1</ram-air-factor>
   <air-intake-impedance-factor> 0 </air-intake-impedance-factor>
   <cooling-factor> 0.36 </cooling-factor>
   <cylinder-head-mass unit="LBS"> 25.28 </cylinder-head-mass>

   <boostmanual> 0 </boostmanual>
   <numboostspeeds>1</numboostspeeds>
   <boostoverride>0</boostoverride>
   <boost-loss-factor> 0 </boost-loss-factor>
   
   <ratedboost1 unit="INHG">6.58</ratedboost1>
   <ratedrpm1>2700</ratedrpm1>
   <ratedpower1>310</ratedpower1>
   <ratedaltitude1>17000</ratedaltitude1>

</piston_engine>

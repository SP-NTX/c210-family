		<model>
			<path>Models/c210n.xml</path>
			<livery>
				<file type="string">Default</file>
			</livery>
		</model>
		<menubar include="gui/c210n-menu.xml"></menubar>
		<help include="c210-help.xml"></help>
		<view n="0">
			<name>Cockpit</name>
			<type>lookfrom</type>
			<internal type="bool">true</internal>
			<config>
				<x-offset-m type="double">-0.3</x-offset-m>
				<y-offset-m type="double">0.65</y-offset-m>
				<z-offset-m type="double">0.3</z-offset-m>
				<pitch-offset-deg>3</pitch-offset-deg>
				<default-field-of-view-deg type="double">65.0</default-field-of-view-deg>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<ground-level-nearplane-m type="double">6f</ground-level-nearplane-m>
				<limits>
					<enabled type="bool">false</enabled>
					<left>
						<heading-max-deg type="double">140</heading-max-deg>
						<x-offset-max-m type="double">0.15</x-offset-max-m>
						<x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
					</left>
					<right>
						<heading-max-deg type="double">140</heading-max-deg>
						<x-offset-max-m type="double">0.15</x-offset-max-m>
						<x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
					</right>
				</limits>
			</config>
			<dynamic>
				<enabled type="bool">false</enabled>
			</dynamic>
		</view>
	</sim>
	
	<nasal>
		<c210>
